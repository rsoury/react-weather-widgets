{
  "name": "basic-es6-linting-config",
  "version": "1.0.0",
  "description": "Basic ES6 Lint/Hint Config",
  "scripts": {
    "start": "npm-run-all --parallel dev lint:watch",
    "dev": "nodemon --delay 1 --ignore \"src/*\" server/app.js --exec \"node --require dotenv/config --require babel-register\"",
    "lint": "node_modules/.bin/esw *.js",
    "lint:watch": "npm run lint -- --watch",
    "build": "cross-env ENV=production webpack --progress",
    "prod": "cross-env ENV=production node --require dotenv/config --require babel-register server/app.js"
  },
  "author": "Ryan Soury",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "colors": "^1.1.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "jsonfile": "^4.0.0",
    "morgan": "^1.9.0",
    "normalize.css": "^7.0.0",
    "path": "^0.12.7",
    "prop-types": "^15.6.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.6",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "react-skycons": "^0.3.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "request": "^2.83.0",
    "superagent": "^3.7.0",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "base64-image-loader": "^1.2.0",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.3",
    "npm-run-all": "^4.1.1",
    "postcss-global-import": "^1.0.1",
    "postcss-loader": "^2.0.8",
    "raw-loader": "^0.5.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "source-map-loader": "^0.2.2",
    "style-loader": "^0.19.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "tracer": "^0.8.11",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0"
  }
}
